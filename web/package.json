{
  "name": "bonds-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . && node scripts/check-i18n-keys.mjs",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "gen:api": "swagger-typescript-api generate -p ../server/docs/swagger.json -o src/api/generated -n api.ts --axios --single-http-client --extract-request-body --extract-response-body --extract-response-error --sort-types --sort-routes --unwrap-response-data --patch --module-name-first-tag --modular && node scripts/fix-generated-imports.mjs"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@simplewebauthn/browser": "^13.2.2",
    "@tanstack/react-query": "^5.90.21",
    "antd": "^6.3.0",
    "axios": "^1.13.5",
    "dayjs": "^1.11.19",
    "filesize": "^11.0.13",
    "i18next": "^25.8.7",
    "i18next-browser-languagedetector": "^8.2.1",
    "lunar-javascript": "^1.7.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "swagger-typescript-api": "^13.2.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
